<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearRowrectangletwentysix"
    style="@style/groupStylebluegray_2"
    android:layout_width="@dimen/_170pxv"
    android:layout_height="@dimen/_60pxv"
    android:layout_gravity="start"
    android:layout_marginStart="@dimen/_12pxh"
    android:layout_marginTop="@dimen/_18pxv"
    android:layout_marginEnd="@dimen/_22pxh"
    android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/linearColumnweightTwo"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_18pxh"
        android:layout_marginTop="@dimen/_1pxv"
        android:layout_marginEnd="@dimen/_5pxh"
        android:layout_marginBottom="@dimen/_1pxv"
        android:layout_weight="0.49"
        android:gravity="start"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txtProd"
            style="@style/txtInterregular15"
            android:layout_width="56dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginEnd="@dimen/_10pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_omo_500g"
            android:textSize="12dp"
            tools:text="@string/lbl_omo_500g" />

        <TextView
            android:id="@+id/txtqnty"
            style="@style/txtInterregular15"
            android:layout_width="56dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_qnt_2"
            android:textSize="12dp"
            tools:text="@string/lbl_qnt_2" />

        <TextView
            android:id="@+id/txtPrice"
            style="@style/txtInterregular15"
            android:layout_width="56dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_marginTop="@dimen/_5pxv"
            android:layout_marginEnd="@dimen/_10pxh"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_price_500"
            android:textSize="12dp"
            tools:text="@string/lbl_price_500" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="110dp"
        android:layout_height="75dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textCode"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Code"
            android:textColor="#000000"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/Total"
            style="@style/txtInterregular15"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginTop="@dimen/_4pxv"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@string/lbl_total_1000"
            android:textSize="12dp"
            tools:text="@string/lbl_total_1000" />
    </LinearLayout>
</LinearLayout>