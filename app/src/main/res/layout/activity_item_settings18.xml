<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".ItemSettings18"
    android:orientation="vertical">
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/prodCategory"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle">
        <ImageView android:id="@+id/imageArrowLeftOne"
            android:layout_gravity="start"
            android:layout_height="@dimen/_25pxh"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_4pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="@dimen/_25pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft1"
            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft1"/>

        <LinearLayout
            android:id="@+id/viewRectangleForty"
            android:layout_width="364dp"
            android:layout_height="219dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_78pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_marginEnd="@dimen/_78pxh"
            android:background="@drawable/rectangle_bg_gray_900_5e_radius_10"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="494dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:src="@drawable/download69" />
        </LinearLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnEmployees"
            style="@style/btnSolidRounded_6"
            android:layout_width="match_parent"
            android:layout_height="49dp"
            android:layout_marginStart="@dimen/_78pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_marginEnd="@dimen/_78pxh"
            android:ellipsize="end"
            android:gravity="center"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_9pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingBottom="@dimen/_9pxv"
            android:singleLine="true"
            android:text="Add Item"
            android:textAllCaps="false"
            tools:text="Add Items" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnAddEmployees"
            style="@style/btnSolidRounded_6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_78pxh"
            android:layout_marginTop="@dimen/_21pxv"
            android:layout_marginEnd="@dimen/_78pxh"
            android:ellipsize="end"
            android:gravity="center"
            android:paddingStart="@dimen/_21pxh"
            android:paddingTop="@dimen/_9pxv"
            android:paddingEnd="@dimen/_17pxh"
            android:paddingBottom="@dimen/_9pxv"
            android:singleLine="true"
            android:text="@string/lbl_add_employees"
            android:textAllCaps="false"
            tools:text="Export Items" />
    </LinearLayout>


</LinearLayout>
