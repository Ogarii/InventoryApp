<?xml version="1.0" encoding="utf-8"?>



        <LinearLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/linearColumnrectangletwentytwo"
            android:layout_width="373dp"
            android:layout_height="88dp"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearRowrectangletwentytwo"
                style="@style/groupStylebluegray_2"
                android:layout_width="341dp"
                android:layout_height="88dp"
                android:gravity="start"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/viewRectangleTwentyTwo"
                    android:layout_width="69dp"
                    android:layout_height="71dp"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginBottom="@dimen/_11pxv"
                    android:background="@color/gray_900_5e"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        tools:src="@tools:sample/avatars" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearColumnweight"
                    android:layout_width="239dp"
                    android:layout_height="88dp"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_138pxh"
                    android:layout_marginBottom="@dimen/_16pxv"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/prodName"
                        style="@style/txtInterregular15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_3pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_omo_500g"
                        android:textSize="12sp"
                        tools:text="@string/lbl_omo_500g" />

                    <TextView
                        android:id="@+id/prodname"
                        style="@style/txtInterregular15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_qnt_2"
                        android:textSize="12sp"
                        tools:text="@string/lbl_qnt_2" />

                    <TextView
                        android:id="@+id/prodPrice"
                        style="@style/txtInterregular15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_price_500"
                        android:textSize="12sp"
                        tools:text="@string/lbl_price_500" />

                    <TextView
                        android:id="@+id/prodCategoryy"
                        style="@style/txtInterregular15"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_total_1000"
                        android:textSize="12sp"
                        tools:text="@string/lbl_total_1000" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
