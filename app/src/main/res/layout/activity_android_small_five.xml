                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_37pxv"
                    android:layout_marginEnd="@dimen/_12pxh">

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerListweightThree"
                        itemSpace="@{@dimen/_7pxh}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top|center"
                        android:layout_marginStart="@dimen/_102pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_102pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:nestedScrollingEnabled="false"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        tools:itemCount="4"
                        tools:listitem="@layout/row_listweight_three" />

                    <LinearLayout
                        android:id="@+id/linearRowrectangletwenty"
                        style="@style/groupStylebluegray_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:orientation="horizontal">

                        <View
