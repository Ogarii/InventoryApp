                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:ellipsize="end"
                                android:gravity="top|start"
                                android:singleLine="true"
                                android:text="@string/lbl_omo_500g"
                                tools:text="@string/lbl_omo_500g" />
