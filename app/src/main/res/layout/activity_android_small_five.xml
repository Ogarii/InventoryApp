                        android:layout_gravity="top|center"
                        android:layout_marginStart="@dimen/_102pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_102pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:orientation="vertical">

                        <FrameLayout
                            android:id="@+id/frameStackweight"
                            android:layout_width="@dimen/_72pxh"
                            android:layout_height="@dimen/_19pxv"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_marginEnd="@dimen/_10pxh">

                            <TextView
                                android:id="@+id/prodName"
                                style="@style/txtInterregular15"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:ellipsize="end"
                                android:gravity="top|start"
                                android:singleLine="true"
                                android:text="@string/lbl_omo_500g"
                                tools:text="@string/lbl_omo_500g" />

                            <TextView
                                android:id="@+id/txtWeightOne"
                                style="@style/txtInterregular15"
