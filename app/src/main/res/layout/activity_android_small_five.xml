                                android:id="@+id/txtLanguageEight"
                                style="@style/txtInterregular15"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@string/lbl_total_1000"
                                tools:text="@string/lbl_total_1000" />
                        </LinearLayout>
                    </LinearLayout>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameStacklanguageNine"
                    android:layout_width="@dimen/_281pxh"
