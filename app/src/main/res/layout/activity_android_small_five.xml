                    android:layout_width="@dimen/_219pxh"
                    android:layout_height="@dimen/_52pxv"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_145pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_7pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_finish"
                    android:textAllCaps="false"
                    tools:text="@string/lbl_finish" />
